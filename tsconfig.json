{
	"compilerOptions" : {
		"target" : "ESNext",
		"module" : "ESNext",
		"esModuleInterop" : true,
		"strict" : true,
		"skipLibCheck" : true,
		"outDir" : "dist",
		"removeComments" : true,
		"moduleResolution": "bundler"
	},
	"include" : [
		"src"
	],
	"exclude" : [
		"node_modules",
		"dist"
	]
}
